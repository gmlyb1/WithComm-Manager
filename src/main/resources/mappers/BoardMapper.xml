<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.soft.service.impl.BoardMapper">  
  
  <select id="getList" parameterType="boardVO" resultType="boardVO">
	  select
	   *
	  from
	  tbl_board;
  </select>
  
  <insert id="insertBoard">
  		insert into tbl_board
  		(
  	      board_idx
  	     ,board_writer
  	     ,board_title
  	     ,board_content
  	     ,board_regdate
  		)
		values
		(
		  #{board_idx}
		 ,#{board_writer}
		 ,#{board_title}
		 ,#{board_content}
		 ,#{board_regdate}
		)  
  </insert>
  
  
  
  
  
  
  
</mapper>
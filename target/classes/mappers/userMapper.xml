<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="our.soft.mapper.userMapper">  
  
  <select id="getIdCnt" parameterType="UserVO" resultType="Integer">
       select
       	count(*)
       from
        tbl_user
       where 
        userId = #{userId}
  </select>
  
  <insert id="insertMember">
  		insert into tbl_member
  		   (
			 userName
			,userId
			,userPw
			,userAuth
			,userEmail
			,userTel
			,userIdYn
			,userLatestLogin
			,userDeleteYn
			,userRegDate
  		   )
  			values
  		   (
	  		 #{userName}
			,#{userId}  		
	  		,#{userPw}
	  		,#{userAuth}
	  		,#{userEmail}
	  		,#{userTel}
	  		,#{userIdYn}
	  		,#{userLatestLogin}
	  		,#{userDeleteYn}
	  		,#{userRegDate}
  		   )
  </insert>
  
  
  
  
  
  
  
  
</mapper>